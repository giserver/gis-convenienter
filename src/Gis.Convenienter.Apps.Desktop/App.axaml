<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Gis.Convenienter.Apps.Desktop.App"
             xmlns:local="using:Gis.Convenienter.Apps.Desktop"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
		<StyleInclude Source="avares://Gis.Convenienter.Apps.Desktop/Styles/Icons.axaml"></StyleInclude>
    </Application.Styles>
</Application>